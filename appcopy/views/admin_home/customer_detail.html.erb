
<!DOCTYPE html>

<html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<title>Template</title>
    
	 <meta name="viewport" content="width=device-width, initial-scale=1">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/bootstrap-responsive.min.css" rel="stylesheet">
    <link href="/css/bootstrap-overrides.css" rel="stylesheet">
	<link href="/inventory/css/reset.css" rel="stylesheet" type="text/css"/>
	<link href="/inventory/css/prettify.css" rel="stylesheet" type="text/css"/>
	<link href="/inventory/css/style.css" rel="stylesheet" type="text/css"/>
	<link href="/inventory/css/tablecloth.css" rel="stylesheet" type="text/css"/>


	
	<!--[if lt IE 9]>
  <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
    <!-- Styles -->
    <style>
	.return { 
background-color:white !important;
color:#563d7c !important;
font-family: 'Oxygen', sans-serif !important;
font-weight: bold;

}
li , p , h4, label, h1, h2, h3{
  font-family: 'Oxygen', sans-serif;
  }
  .table-stats thead tr th{ 
 font-family: 'Oxygen', sans-serif !important;
 
 }
	</style>
</head>
<body class="page-header-fixed">
	<%= render :partial => 'header' %>

	<!-- END HEADER -->   
            <%= render :partial => 'sidebar' %>
	
	
	

<section id="content">
	<div class="container">    
		<div class="row">
		<div class="clear">&nbsp;</div>
	</div>
    
    
    
    <div class="row">
		<div class="span3">
		</div>
		<div class="span8">
	
		<h2 style="color: #563d7c;"> Customer Details</h2>
	
		</div>
    </div>
      
    <div class="row">
		<div class="span3">
	
	</div>
    <div class="span8">
        <div class="hidden-phone">
        <table class="table table-stats table-bordered table-striped" id="example-default">
            <thead>
				<tr>
				  
				  <th class="left"> <h4>Customer Id</h4></th>
				  <th class="left"> <h4>Customer Name</h4></th>
				  <th class="left"><h4>Email</h4></th>
				  <th class="left"><h4>Phone</h4></th>
				  
				  <th class="left"><h4>Order Placed</h4></th>
				  <th class="left"><h4>Last Order Date</h4></th>
				  
				  <th class="left"><h4>Total Purchase</h4></th>
				  <th class="left"><h4>Address</h4></th>
				  <th class="left"><h4>City</h4></th>
				  <th class="left"><h4>Country</h4></th>
				  
				</tr>
			</thead>
            <tbody>
					<% @d =Masterusers.find_by_sql("select * from sub_users_"+session[:master_user_id].to_s )%>
					<%@d.each do |i|%>
						<tr>
							<td class="left"> <%= i["sub_user_id"]%></td>
							<td class="left"><%= i["name"]%><%= i["last_name"]%></td>
							<td class="left"><%= i["email"]%></td>
							<td class="left"><%= i["phone"]%></td>
							
							<td class="left"><%= i["order_id"]%></td>
							<td class="left"><%= i["last_order_date"]%></td>
							<td class="left"><%= i["total"]%></td>
							<td class="left"><%= i["address"]%></td>
							<td class="left"><%= i["city"]%></td>
							<td class="left"><%= i["country"]%></td>
							
							
						</tr>
					<%end%>
			</tbody>
        </table>
	</div>
		

 
          
</section>

<script type="text/javascript" charset="utf-8">
  $(document).ready(function() {
    
    window.prettyPrint && prettyPrint();
    
 
    
   
    
  });
</script>



</body>
<script src="/first/js/bootstrap-tooltip.js"></script>
		<script src="/first/js/bootstrap-popover.js"></script>
</html>
