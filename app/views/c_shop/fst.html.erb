    <!-- Sign In Option 1 -->
    <div id="sign_up1">
        <div class="container">
            <div class="row">
                <div class="span12 header">
                    <h4>Create your shop</h4>
                    <p>
                        Text Here</p>

                    <div class="span4 social">
                        <a href="sign-up.html#" class="circle facebook">
                            <img src="img/face.png" alt="">
                        </a>
                         <a href="sign-up.html#" class="circle twitter">
                            <img src="img/twt.png" alt="">
                        </a>
                         <a href="sign-up.html#" class="circle gplus">
                            <img src="img/gplus.png" alt="">
                        </a>
                    </div>
                </div>

                <div class="span3 division">
                    <div class="line l"></div>
                    <span>or</span>
                    <div class="line r"></div>
                </div>

                <div class="span12 footer">
                    <%= form_tag("/c_shop/master_signup/", :method=>"put") do %>
                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
						<%=text_field_tag 'name', nil, :placeholder => 'Shop Name'%>
                        <%=text_field_tag 'email', nil, :placeholder => 'Email'%>
                        <%= submit_tag("upload",  :value => "upload", :class => "btn btn-large btn btn-info" ,  :onclick => "alert('Signup successfuly!')") %>
                    <% end %>
                </div>

                <!-- <div class="span5 remember">
                    <label class="checkbox">
