						<%= link_to image_tag("/ja/images/twitter.jpg", :width => 85, :height => 60), :id => @profile,:name => "Residential", :method => :post%>
					   </div><!--span-->
                                    </div>

				</div><!--outer span-->
							
				</div> <!--tab pane-->
	
				<div class="tab-pane" id="tab2">
							  
							<div class="span12"><div class="row-fluid">
								 <div class="span4">
						                          <label class="control-label"  for="name">Full Name</label>
											<%= text_field_tag(:name,"",:required=> true) %>
					                        </div>
								<div class="span4">	
									   <label class="control-label"  for="name">Address</label>
						        		   <TEXTAREA NAME="address" COLS=40 ROWS=3></TEXTAREA>
					    			</div>
								<div class="span4">
									    <label class="control-label" for="name">Pin Code</label>
										<%= text_field_tag(:pincode,"",:required=> true) %>
					    			</div>
							   </div>	
							</div>
							<div class="span12" style="margin:0;padding:0;"><div class="row-fluid">
								<div class="span4">
										<label class="control-label"  for="country">State</label>
						      				  <%= text_field_tag(:state,"",:required=> true) %>
					   			 </div>
								<div class="span4">					
						      			  <label class="control-label"  for="country">City</label>
						       			 <%= text_field_tag(:city,"",:required=> true) %>
					    			</div>
								<div class="span4">
						      			  <label class="control-label"  for="country">Land Mark</label>
						      		  <%= text_field_tag(:landmark,"",:required=> true) %>
								</div></div>
							</div>
							<div class="span12" style="margin:0;padding:0;"><div class="row-fluid">
								<div class="span4">
						        		<label class="control-label" for="country">Country</label>
						        		<%= text_field_tag(:country,"",:required=> true) %>
					   			 </div>
								<div class="span4">					
						      			  <label class="control-label" for="phone">Phone</label>
						        		<%= text_field_tag(:phone,"",:required=> true) %>
					    			</div>
                                                                  </div>
							</div>
					  	
				</div>
			    
