					  <thead>
						  <tr>
							
							<th>Image</th>
							<th>Item Name</th>
							<th>Quantity</th>
							<th>Unit Price</th>
							<th>Total</th>
						  </tr>
						</thead>
						<tbody>
							<%@product=Masterusers.find_by_sql(["select * from product_"+session[:master_user_id].to_s+" where product_id IN (?)",@a])%>
						
					<%@p.each do |a|%>
						<%@product.each do |i|%>
							<%b=a.split("-")%>
							<%if i['product_id']==b[0].to_i  then %>
								<%if i['revised_price'].nil? then %>
									<%@p_total= i['sale_price'].to_i * b[1].to_i%>
									<%@total=@total.to_i + @p_total %>
								<%else%>
									<%@p_total= i['sale_price'].to_i * b[1].to_i%>
									<%@total=@total.to_i + @p_total%>
								<%end%>

				  <tr>




					<td class="cntr"><a href="#"><img src="<%="#{i["image_1"]}/thumb/#{i["image_name_1"]}"%>" style="height: 100px!important;width: 143px!important;" alt="#"></a></td>
					<td><%=i[:title]%> </td>
					<td><%=b[1]%></td>
					<td>$<%=i["sale_price"]%></td>
					<td>$<%=@p_total%></td>
				  </tr>			  
				<%end%>
							
						<%end%>
					<%end%>
                        
				  <tr>
					<td>&nbsp;</td>
					<td>&nbsp;</td>
					<td>&nbsp;</td>
					<td>&nbsp;</td>
					<td>Total:</td>
					<td><strong>$<%=@total%></strong></td>
				  </tr>		  
				</tbody>
			  </table>
				
	
